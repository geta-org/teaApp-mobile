{
  "name": "native_template_styled",
  "version": "0.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prepare": "husky install && chmod ug+x .husky/* && chmod ug+x .git/hooks/* && npm install --global eslint",
    "pre-commit": "lint-staged",
    "commit": "git-cz",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@expo-google-fonts/roboto": "^0.2.2",
    "@react-navigation/native": "^6.0.16",
    "@react-navigation/native-stack": "^6.9.4",
    "axios": "^1.2.0",
    "expo": "~47.0.12",
    "expo-font": "~11.0.1",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "phosphor-react-native": "^1.1.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "13.4.0",
    "react-native-svg-transformer": "^1.0.0",
    "styled-components": "^5.3.6",
    "expo-splash-screen": "~0.17.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@rocketseat/eslint-config": "^1.1.3",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "@types/styled-components-react-native": "^5.2.0",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "react-native-dotenv": "^3.4.6",
    "typescript": "^4.6.3"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
